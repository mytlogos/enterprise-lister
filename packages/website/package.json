{
  "name": "enterprise-website",
  "version": "1.16.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "lint:website": "eslint packages/website/",
    "test": " LOG_LEVEL=error node ./node_modules/mocha/bin/mocha --ui bdd ./dist/**/test/database/**/*.js ./dist/**/test/*.js",
    "commit": "npx git-cz",
    "lintfix": "eslint --fix '*/**/*.{js,ts,vue}'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx --no-install commitlint --edit $1"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.4",
    "jquery": "^3.6.0",
    "mitt": "^2.1.0",
    "popper.js": "^1.16.1",
    "tslib": "^2.1.0",
    "vue": "^3.0.9",
    "vue-observe-visibility": "^1.0.0",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@commitlint/prompt": "^12.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@types/bootstrap": "^4.5.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chart.js": "^2.9.31",
    "@types/copy-webpack-plugin": "^6.4.1",
    "@types/expect.js": "^0.3.29",
    "@types/jquery": "^3.5.5",
    "@types/mocha": "^8.2.2",
    "@types/sinon": "^9.0.11",
    "@types/sinon-as-promised": "^4.0.14",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-plugin-typescript": "^4.5.12",
    "@vue/cli-plugin-vuex": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/compiler-sfc": "^3.0.9",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "6.26.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^5.2.0",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2",
    "sinon": "^10.0.0",
    "sinon-as-promised": "^4.0.3",
    "sinon-chai": "^3.6.0",
    "typescript": "^4.2.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
