{
  "name": "enterprise-other",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pm2": "pm2 start",
    "pm2:refresh": "pm2 restart ecosystem.config.js --update-env",
    "start": "node server/dist/startServer.js",
    "start:server": "node server/dist/startServer.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "build:live": "nodemon --watch 'server/**/*.ts' --exec ts-node server/bin/startServer.ts",
    "buildServer": "tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "brotli": "^1.3.2",
    "cheerio": "^1.0.0-rc.3",
    "cloudscraper": "^4.3.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "latest",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.0",
    "dotenv": "^8.1.0",
    "emoji-strip": "latest",
    "express": "^4.17.1",
    "feedparser-promised": "^2.0.1",
    "helmet": "^3.21.2",
    "htmlparser2": "^4.0.0",
    "http-errors": "^1.7.3",
    "localtunnel": "^2.0.0",
    "monitor": "^0.6.10",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "node-cache": "^5.0.2",
    "npm": "^6.13.1",
    "object-hash": "latest",
    "parse5-parser-stream": "latest",
    "promise-mysql": "^4.1.1",
    "read-last-lines": "latest",
    "request-promise-native": "^1.0.8",
    "stringify-stream": "^1.0.5",
    "throttle-exec": "^3.1.0",
    "throttle-function": "^0.1.0",
    "tough-cookie": "latest",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "validate.js": "^0.13.1",
    "vue": "latest",
    "vue-class-component": "^6.0.0",
    "vue-ctk-date-time-picker": "^2.1.0",
    "vue-observe-visibility": "^0.4.4",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.1.3",
    "vuex": "latest",
    "websocket": "^1.0.29",
    "winston": "latest"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/cheerio": "^0.22.13",
    "@types/compression": "^1.0.1",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "latest",
    "@types/expect.js": "latest",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/htmlparser2": "^3.10.1",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^24.0.23",
    "@types/localtunnel": "^1.9.0",
    "@types/mocha": "latest",
    "@types/morgan": "^1.7.37",
    "@types/mysql": "latest",
    "@types/node": "12.7.12",
    "@types/node-cache": "latest",
    "@types/parse5-parser-stream": "^5.0.1",
    "@types/request-promise-native": "^1.0.17",
    "@types/uuid": "^3.4.6",
    "@types/websocket": "1.0.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "latest",
    "babel-core": "7.0.0-bridge.0",
    "expect.js": "latest",
    "jest": "latest",
    "mocha": "latest",
    "mocha-parallel-tests": "^2.2.2",
    "nodemon": "^2.0.1",
    "ts-jest": "latest",
    "typescript": "^3.6.2",
    "vue-template-compiler": "latest"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  }
}
