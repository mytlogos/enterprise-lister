{
  "name": "enterprise-other",
  "version": "1.16.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:server && npm run build:website",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "npm run lint:server && npm run lint:website",
    "build:server": "tsc -p src/server/",
    "build:website": "vue-cli-service build",
    "build:website:dev": "vue-cli-service build --mode development",
    "lint:server": "eslint -c src/server/.eslintrc.js src/server/bin/",
    "lint:website": "eslint src/website/",
    "pm2": "pm2 start",
    "pm2:refresh": "pm2 restart ecosystem.config.js --update-env",
    "start": "node dist/server/startServer.js",
    "start:server": "node dist/server/startServer.js",
    "start:crawler": "node dist/server/startCrawler.js",
    "test": " LOG_LEVEL=error node ./node_modules/mocha/bin/mocha --ui bdd ./dist/**/test/database/**/*.js ./dist/**/test/*.js",
    "migrate": "node dist/server/misc/migrate.js",
    "commit": "npx git-cz",
    "generate:api": "node dist/server/misc/openapi/generate.js",
    "lintfix": "eslint --fix '*/**/*.{js,ts,vue}'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx --no-install commitlint --edit $1"
    }
  },
