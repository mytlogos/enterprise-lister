{
  "name": "enterprise-other",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pm2": "pm2 start",
    "pm2:refresh": "pm2 restart ecosystem.config.js --update-env",
    "start": "node server/dist/startServer.js",
    "start:server": "node server/dist/startServer.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "build:live": "nodemon --watch 'server/**/*.ts' --exec ts-node server/bin/startServer.ts",
    "buildServer": "tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "cheerio": "^1.0.0-rc.2",
    "cloudscraper": "^4.1.2",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "latest",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.0",
    "dotenv": "^8.0.0",
    "emoji-strip": "latest",
    "express": "^4.17.1",
    "feedparser-promised": "^2.0.0",
    "helmet": "^3.18.0",
    "htmlparser2": "latest",
    "http-errors": "^1.7.2",
    "morgan": "^1.9.1",
    "mysql": "latest",
    "node-cache": "latest",
    "object-hash": "latest",
    "parse5-parser-stream": "latest",
    "promise-mysql": "latest",
    "request-promise-native": "^1.0.5",
    "tough-cookie": "latest",
    "ts-node": "^8.3.0",
    "uuid": "^3.3.2",
    "validate.js": "^0.13.1",
    "vue": "^2.5.21",
    "vue-class-component": "^6.0.0",
    "vue-ctk-date-time-picker": "^2.0.6",
    "vue-observe-visibility": "^0.4.3",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "websocket": "^1.0.28",
    "winston": "latest",
    "read-last-lines": "latest"
  },
  "devDependencies": {
    "@types/parse5-parser-stream": "^5.0.1",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/cheerio": "^0.22.10",
    "@types/compression": "0.0.36",
    "@types/connect-history-api-fallback": "^1.3.1",
    "@types/debug": "0.0.31",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.42",
    "@types/htmlparser2": "^3.10.0",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^23.1.4",
    "@types/morgan": "^1.7.35",
    "@types/node": "11.9.5",
    "@types/request-promise-native": "^1.0.15",
    "@types/uuid": "^3.4.4",
    "@types/validate.js": "latest",
    "@types/websocket": "0.0.40",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-e2e-cypress": "^3.3.0",
    "@vue/cli-plugin-typescript": "^3.3.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "nodemon": "^1.18.9",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  }
}
