{
  "name": "enterprise-other",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pm2": "pm2 start",
    "pm2:refresh": "pm2 restart ecosystem.config.js --update-env",
    "start": "node server/dist/startServer.js",
    "start:server": "node server/dist/startServer.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "build:live": "nodemon --watch 'server/**/*.ts' --exec ts-node server/bin/startServer.ts",
    "buildServer": "tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "brotli": "^1.3.2",
    "cheerio": "^1.0.0-rc.3",
    "cloudscraper": "^4.6.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "latest",
    "cookie-parser": "^1.4.5",
    "debug": "^4.1.0",
    "dotenv": "^8.2.0",
    "emoji-strip": "latest",
    "express": "^4.17.1",
    "feedparser-promised": "^2.0.1",
    "helmet": "^3.21.3",
    "htmlparser2": "^4.1.0",
    "http-errors": "^1.7.3",
    "localtunnel": "^2.0.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-cache": "^5.1.0",
    "object-hash": "^2.0.3",
    "parse5-parser-stream": "^5.1.1",
    "promise-mysql": "^4.1.3",
    "read-last-lines": "^1.7.2",
    "request-promise-native": "^1.0.8",
    "stringify-stream": "^1.0.5",
    "throttle-exec": "^3.1.0",
    "throttle-function": "^0.1.0",
    "tough-cookie": "^4.0.0",
    "ts-node": "^8.8.1",
    "tslib": "^1.11.1",
    "uuid": "^7.0.2",
    "validate.js": "^0.13.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-ctk-date-time-picker": "^2.4.0",
    "vue-observe-visibility": "^0.4.6",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cheerio": "^0.22.17",
    "@types/compression": "^1.7.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/expect.js": "^0.3.29",
    "@types/express": "^4.17.3",
    "@types/helmet": "0.0.45",
    "@types/htmlparser2": "^3.10.1",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.1.4",
    "@types/localtunnel": "^2.0.0",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.0",
    "@types/mysql": "^2.15.9",
    "@types/node": "13.9.3",
    "@types/node-cache": "^4.2.5",
    "@types/parse5-parser-stream": "^5.0.1",
    "@types/request-promise-native": "^1.0.17",
    "@types/sinon": "^7.5.2",
    "@types/sinon-as-promised": "^4.0.12",
    "@types/sinon-chai": "^3.2.3",
    "@types/uuid": "^7.0.2",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-typescript": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-core": "6.26.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "expect.js": "^0.3.1",
    "jest": "^25.1.0",
    "mocha": "^7.1.1",
    "mocha-parallel-tests": "^2.3.0",
    "nodemon": "^2.0.2",
    "sinon": "^9.0.1",
    "sinon-as-promised": "^4.0.3",
    "sinon-chai": "^3.5.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  }
}
